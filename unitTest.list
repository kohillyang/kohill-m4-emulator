## 0x788,lsr carry test
	mov r0,#2
	mov r1,#5
	lsl r0,r1,#30
result: 
	r0=0x40000000
	r1=0x5
	cpsr=0x20000173
## 0x7c8 ,lsr no carry test
	mov r0,#2
	mov r1,#5
	lsl r0,r1,#31
result:
	r0=0x800000004
	r1=0x05
	cpsr=0x80000173
## 0x4088 ,lsr register no carry test	
	r0 = 2;
	r1 = 5;
	r0=0x40
	r1=0x05
	cpsr=0x00000173
